<h1>Create a Team and Heroes!</h1>
<form method="POST" action="/teams">
    <p>team name <input type="text" id="team_name" name="team[name]"></p>
    <p>team motto <input type="text" id="team_name" name="team[motto]"></p>
    
    <h1>Hero 1</h1>
    <p>name <input type="text" id="member1_name" name="team[members][][name]"></p>
    <p>power <input type="text" id="member1_power" name="team[members][][power]"></p>
    <p>biography<input type="text" id="member1_bio" name="team[members][]][bio]"></p>
    <h1>Hero 2</h1>
    <p>name <input type="text" id="member2_name" name="team[members][][name]"></p>
    <p>power <input type="text" id="member2_power" name="team[members][][power]"></p>
    <p>biography<input type="text" id="member2_bio" name="team[members][]][bio]"></p>
    <h1>Hero 3</h1>
    <p>name <input type="text" id="member3_name" name="team[members][][name]"></p>
    <p>power <input type="text" id="member3_power" name="team[members][][power]"></p>
    <p>biography<input type="text" id="member3_bio" name="team[members][]][bio]"></p>
    <input type="submit" value="submit">
</form>
